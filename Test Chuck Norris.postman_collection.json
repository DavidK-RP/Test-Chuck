		{
			"name": "Filtro de broma por texto",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Returns 200 OK status\", () => {",
							"    pm.response.to.have.status(200)",
							"})",
							"",
							"pm.test(\"Response with selected text\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.result).to.be.an(\"array\").that.is.not.empty;",
							"    jsonData.result.forEach(function (joke) {",
							"        pm.expect(joke.value.toLowerCase()).to.include(\"beard\");",
							"    });",
							"",
							"});",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"token\": \"{{token}}\"\n}",
